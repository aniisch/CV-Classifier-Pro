{
  "name": "cv-classifier-pro",
  "version": "1.0.0",
  "description": "Application desktop pour analyser et classifier des CVs",
  "private": true,
  "main": "electron/main.js",
  "author": "CV Classifier Pro",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "date-fns": "^2.30.0",
    "html2pdf.js": "^0.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "vite": "^5.0.7",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "electron-dev": "concurrently \"vite --port 5173\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-build": "vite build && electron-forge package",
    "make": "vite build && electron-forge make",
    "make:win": "vite build && electron-forge make --platform win32",
    "make:mac": "vite build && electron-forge make --platform darwin",
    "make:linux": "vite build && electron-forge make --platform linux"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
